{% extends 'base.html.twig' %}

{% block title %}Homepage - {{ parent() }}{% endblock %}

{% block stylesheets %}

{% endblock %}

{% block body %}
    {% for review in restaurant.reviews %}
        {{ dump(review.comment) }}
    {% endfor %}
    <hr>
    {% for meal in restaurant.meals %}
        {{ dump(meal.foodName) }}
    {% endfor %}
    <hr>
    {% for singleRestaurant in restaurants %}
        {{ dump(singleRestaurant.name) }}
    {% endfor %}
{% endblock %}
